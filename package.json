{
  "name": "bearcar-api",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.js",
  "type": "commonjs",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "STAGE=test jest --coverage --runInBand ./test",
    "start": "fastify start -l info app.js",
    "start-db": "docker compose rm -f && docker compose pull && docker compose up --build -d",
    "db-up": "STAGE=test knex migrate:up",
    "db-make-migration": "knex migrate:latest",
    "dev": "fastify start -w -l info -P app.js",
    "prepare": "husky install",
    "deploy-api-gw": "fastify start ./scripts/api-gateway.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/jwt": "^6.3.3",
    "ajv": "^8.11.2",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.5.1",
    "fastify-plugin": "^4.0.0",
    "knex": "^2.3.0",
    "mysql2": "^2.3.3"
  },
  "devDependencies": {
    "@fastify/swagger": "^8.2.1",
    "husky": ">=6",
    "jest": "^29.3.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  }
}
